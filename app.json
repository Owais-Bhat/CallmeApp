      "package": "com.owais43.NewApp",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "READ_CONTACTS"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "owner": "owais43",
    "plugins": [
      "react-native-video"
    ],
    "extra": {
      "eas": {
        "projectId": "8b2912b0-3a2e-4295-b0cd-db09630ea0a2"
      }
    },
    "runtimeVersion": {
